{
  "family": "devops-practice-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::941377126042:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "devops-practice-container",
      "image": "941377126042.dkr.ecr.eu-central-1.amazonaws.com/devops-practice-app:latest",
      "memory": 512,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
